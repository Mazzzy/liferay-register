		<property access="com.liferay.portal.dao.orm.hibernate.LiferayPropertyAccessor" name="bdate" type="org.hibernate.type.TimestampType" />
		<property access="com.liferay.portal.dao.orm.hibernate.LiferayPropertyAccessor" name="email" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
</hibernate-mapping>