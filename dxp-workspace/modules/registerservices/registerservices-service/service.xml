<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.3.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_3_0.dtd">

<service-builder dependency-injector="ds" package-path="com.liferay.register.services" auto-namespace-tables="false">
	<namespace>Registration</namespace>
	<entity name="Register" local-service="true" remote-service="false">
        <column name="rid" type="long" primary="true"></column>
        <column name="name" type="String"></column>
        <column name="surname" type="String"></column>
        <column name="bdate" type="Date"></column>
        <column name="email" type="String"></column>
    </entity>
</service-builder>